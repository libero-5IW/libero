    "@tiptap/vue-3": "^2.11.7",
    "axios": "^1.8.4",
    "html2pdf.js": "^0.10.3",
    "i18n-iso-countries": "^7.14.0",
    "pinia": "^3.0.1",
    "tailwindcss": "^4.0.15",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vue3-toastify": "^0.2.8",
    "vuedraggable": "^4.1.0",
    "vuetify": "^3.7.18",
