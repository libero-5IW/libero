    "eslint-plugin-vue": "~10.0.0",
    "jiti": "^2.4.2",
    "npm-run-all2": "^7.0.2",
    "rollup": "^3.29.4",
    "start-server-and-test": "^2.0.10",
    "typescript": "~5.8.0",
